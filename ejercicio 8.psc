Proceso inicio_area_triangulo
	escribir "introduzca lado 1 "
	leer a
	escribir "introduzca lado 2 "
	leer b
	escribir " introduzaca lado 3"
	leer c
	semiperimetro<-(a+b+c)/2
	escribir " el semiperimetro es=" semiperimetro
